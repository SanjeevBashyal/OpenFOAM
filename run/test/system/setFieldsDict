FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.sediment 0
);

regions
(
    boxToCell
    {
        box ( 0.2 0.2 0.2) (0.4 0.4 0.4);

        fieldValues
        (
            volScalarFieldValue alpha.sediment 1
        );
    }
);


